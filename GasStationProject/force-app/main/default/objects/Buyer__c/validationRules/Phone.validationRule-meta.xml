<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( Phone_Number__c , &quot;[6-9]{1}[0-9]{9}&quot;))</errorConditionFormula>
    <errorDisplayField>Phone_Number__c</errorDisplayField>
    <errorMessage>incorrect data</errorMessage>
</ValidationRule>
